{
  "name": "vue-camera-gestures",
  "version": "2.0.0-alpha.5",
  "description": "Let users control your Vue app using AI and their camera in just 1 line of HTML!",
  "files": [
    "dist",
    "dist-plain"
  ],
  "main": "./dist/vue-camera-gestures.umd.js",
  "module": "./dist/vue-camera-gestures.es.js",
  "unpkg": "dist/vue-camera-gestures.iife.js",
  "exports": {
    ".": {
      "import": "./dist/vue-camera-gestures.es.js",
      "require": "./dist/vue-camera-gestures.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && vite build --config vite-no-dependencies.config.js",
    "serve": "vite preview",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danielelkington/vue-camera-gestures.git"
  },
  "keywords": [
    "vue",
    "vuejs",
    "camera",
    "ai"
  ],
  "author": "Daniel Elkington",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/danielelkington/vue-camera-gestures/issues"
  },
  "homepage": "https://vue.cameragestures.com/",
  "dependencies": {
    "node-fetch": "^2.6.1",
    "vue": "^3.0.5"
  },
  "devDependencies": {
    "@tensorflow-models/knn-classifier": "^1.2.2",
    "@tensorflow-models/mobilenet": "^2.0.4",
    "@tensorflow/tfjs": "^1.7.0",
    "@vitejs/plugin-vue": "1.1.4",
    "@vue/compiler-sfc": "3.0.5",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-vue": "^7.0.0-beta.4",
    "prettier": "^2.2.1",
    "vite": "2.0.0-beta.69",
    "vitepress": "^0.12.0"
  },
  "peerDependencies": {
    "@tensorflow-models/knn-classifier": "^1.2.2",
    "@tensorflow-models/mobilenet": "^2.0.4",
    "@tensorflow/tfjs": "^1.7.4"
  }
}
